<xml>
  <name type="snde" parenttype="????">SoundEnvironment</name>
  <platform>Halo2</platform>
  <plugin>
    <struct name="SoundEnvironmentBlock">
      <value type="Pad" name="_unnamed0" length="4" />
      <value type="ShortInteger" name="priority" disabled="false">
        <tooltip>when multiple listeners are in different sound environments in split screen, the combined environment will be the one with the highest priority.</tooltip>
      </value>
      <value type="Pad" name="_unnamed1" length="2" />
      <value type="Real" name="room intensity" disabled="false" unit="dB">
        <tooltip>intensity of the room effect</tooltip>
      </value>
      <value type="Real" name="room intensity hf" disabled="false" unit="dB">
        <tooltip>intensity of the room effect above the reference high frequency</tooltip>
      </value>
      <value type="Real" name="room rolloff 0 to 10" disabled="false">
        <tooltip>how quickly the room effect rolls off, from 0.0 to 10.0</tooltip>
      </value>
      <value type="Real" name="decay time 1 to 20" disabled="false" unit="seconds" />
      <value type="Real" name="decay hf ratio 1 to 2" disabled="false" />
      <value type="Real" name="reflections intensity" disabled="false" unit="dB[-100,10]" />
      <value type="Real" name="reflections delay 0 to 3" disabled="false" unit="seconds" />
      <value type="Real" name="reverb intensity" disabled="false" unit="dB[-100,20]" />
      <value type="Real" name="reverb delay 0 to 1" disabled="false" unit="seconds" />
      <value type="Real" name="diffusion" disabled="false" />
      <value type="Real" name="density" disabled="false" />
      <value type="Real" name="hf reference20 to 20000" disabled="false" unit="Hz">
        <tooltip>for hf values, what frequency defines hf, from 20 to 20,000</tooltip>
      </value>
      <value type="Pad" name="_unnamed2" length="16" />
    </struct>
  </plugin>
</xml>